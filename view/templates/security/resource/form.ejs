<%
  let options = [];
  let resTypes = securityParams.resourceTypes || {};
  for (let type of Object.keys(resTypes)) {
    let item = resTypes[type];
    if (item) {
      options.push({
        value: type === '*' ? '' : type,
        label: item.title
      });
    }
  }
  if (options.length === 0) {
      options.push({value: '', label: t('Общие')});
  }
%>

<% layout('../../crud/object') -%>

<% script(`/${module}/js/object/manager.js`) -%>
<% script(`/${module}/js/security/resource/object.js`) -%>

<div class="form-group">
  <label class="col-sm-2 control-label"><%=t('Форматы идентификаторов ')%></label>
  <div class="col-sm-10">
    <div class="form-control-static">
      <ul>
        <li><%=t('Узел навигации ')%>- <b>n:::namespace@code</b></li>
        <li><%=t('Класс ')%>- <b>c:::classname@namespace</b></li>
        <li><%=t('Объект ')%>- <b>i:::classname@namespace@id</b></li>
        <li><%=t('Атрибут ')%>- <b>a:::classname@namespace.propertyName</b></li>
      </ul>
    </div>
  </div>
</div>

<%- partial('../../attrs/_input', { attr: 'id', title: t('Идентификатор'), required:'required' }) %>

<%- partial('../../attrs/_input', { attr: 'name', title: t('Название'), required:'' }) %>
