{
  "parametrised": true,
  "sysTitle": "[[sys.title]]",
  "environment": "[[sys.env]]",
  "addressCatalog": {
    "path": "addressCatalog",
    "feedbackStep": "1000"
  },
  "dashboard": {
    "modules": {
      "dashboard": {
        "config": {
          "ionadmin": {
            "root": "@iondv/ionadmin/modules/dashboard"
          }
        }
      }
    }
  },
  "profiling": {
    "slowQuery": {
      "sources": [
          {
            "collection": "system.profile"
          }
      ]
    }
  },
  "di" : {
    "backupProvider": {
      "module": "@iondv/ionadmin/backend/backupProvider",
      "initMethod": "init",
      "initLevel": 2,
      "options": {
        "backupDir": "../ion-backups",
        "zlib": {
          "level": 1
        },
        "dataRepo": "ion://dataRepo",
        "metaRepo": "ion://metaRepo",
        "bg": "ion://background",
        "workflows": "ion://workflows",
        "sequences": "ion://sequenceProvider",
        "log": "ion://sysLog",
        "accounts": "ion://accounts",
        "accessManager": "ion://roleAccessManager",
        "step": "[[backup.step]]",
        "maxOldMemory": "[[backup.maxOldMemory]]"
      }
    },
    "changelogFactory": {
      "module": "@iondv/commons/lib/changelogger/DsChangelogFactory",
      "options": {
        "dataSource": "ion://Db"
      }
    },
    "schedulerAgent": {
      "module": "@iondv/commons/lib/SchedulerAgent",
      "options": {
        "settings": "ion://settings",
        "repo": "ion://kvRepo"
      }
    }
  }
}